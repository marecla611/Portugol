programa
{
	inclua biblioteca Matematica --> mat
	
	funcao inicio()
	{
		real numero
		real raiz

		numero = 4.0
		raiz = mat.raiz(numero, 2.0) 
		
		escreva("A raíz quadrada de ", numero , " é: ", raiz, "\n")

		numero = 27.0
		raiz = mat.raiz(numero, 3.0) 

		escreva("A raíz cúbica de ", numero , " é: ", raiz, "\n")
	}
}
