programa
{
	inclua biblioteca Matematica --> m
	
	funcao inicio()
	{
		
real numero1, numero2, maior

		escreva("Informe o primeiro número: ")
		leia(numero1)

		escreva("Informe o segundo número: ")
		leia(numero2)

		maior = m.maior_numero(numero1, numero2)

		escreva("\nO maior número é: ", maior, "\n")

	}
}
