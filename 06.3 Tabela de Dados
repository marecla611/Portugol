programa
{
	funcao inicio(cadeia parametros[])
	{
		
cadeia nome[] = { "Katy", "Ariana" , "Demi" }
		real altura[] = { 1.70, 1.48, 1.55 }
		inteiro idade[] = { 32, 21, 23 }

		escreva ("-----------------------------------\n")
		escreva ("               TABELA              \n")
		escreva ("-----------------------------------\n")

		para (inteiro posicao = 0; posicao < 5; posicao++)
		{
			escreva (nome[posicao], "\t\t", altura [posicao], "\t\t", idade[posicao], "\n")

		}



	}
}
