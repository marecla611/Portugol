programa
{
	funcao inicio()
	{
		logico resLogic
		inteiro a, b

		a = 10
		b = 3
		
    resLogic = a > b
		escreva(" a > b ? ", resLogic)

		resLogic = a < b
		escreva("\na < b ? ", resLogic)

    se( a > b )
		escreva("\nA é maior que B") 

		se( a < b )
		escreva("\nA é menor que B") 

		se ( a == b )
		escreva("\nA é igual a B") 
		
		se ( a != b )
		escreva("\nA é diferente de B")
	}
}
